{
  "author": "Tom Rees",
  "name": "open-budget-explorer",
  "description": "Online explorer and reporting tool for the Open Budget Survey results",
  "version": "1.0.0",
  "homepage": "http://internationalbudget.org",
  "repository": {
    "type": "git",
    "url": "http://github.com/okfn/dpkg-ibp"
  },
  "engines": {
    "node": "^8.17.0",
    "npm": "^6.11.3"
  },
  "scripts": {
    "start": "node bin/www",
    "build:dev:explorer": "webpack --config webpack.config.development.js",
    "build:dev:explorer:watch": "webpack --config webpack.config.development.js --watch",
    "build:dev:tracker": "gulp build:dev:tracker",
    "build:dev:tracker:watch": "gulp tracker-watch",
    "build:questionnaires": "node data/questionnaire/build_country_questionnaires.js",
    "build:questionnaires:dev": "node data/questionnaire/build_country_questionnaires.js --dev",
    "build:prod:explorer": "webpack --config webpack.config.production.js",
    "build:prod:tracker": "gulp build:prod:tracker",
    "build:dev:watch": "gulp dev:watch",
    "build:dev": "npm run build:dev:explorer && npm run build:dev:tracker && npm run build:questionnaires:dev",
    "build:prod": "npm run build:prod:explorer && npm run build:prod:tracker && npm run build:questionnaires",
    "build:prod:start": "npm run build:prod && npm run start",
    "test": "NODE_ENV=test istanbul cover _mocha --report lcovonly --check-coverage -- --require babel-core/register -R spec tests/ --harmony",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "extract-pot": "extract-pot --locale tracker/i18n --exclude node_modules --exclude _build -t jinja -f html .",
    "merge-po": "merge-po tracker/i18n",
    "compile-json": "compile-json tracker/i18n tracker/i18n",
    "heroku-postbuild": "npm run build:prod"
  },
  "dependencies": {
    "async": "^2.0.0",
    "babel-loader": "^6.2.4",
    "backbone": "^1.3.3",
    "body-parser": "^1.19.0",
    "bootstrap": "3.4.1",
    "cookie-parser": "^1.4.4",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "debug": "~0.7.4",
    "del": "0.1.1",
    "dotenv": "^2.0.0",
    "exceljs": "^1.6.0",
    "expect.js": "0.1.2",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "express": "^4.14.0",
    "express-basic-auth": "^1.0.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fastparse": "^1.1.1",
    "file-loader": "^0.9.0",
    "floatthead": "^2.0.3",
    "gulp": "3.8.7",
    "gulp-clean": "0.3.1",
    "gulp-concat": "^2.6.1",
    "gulp-extend": "^0.3.0",
    "gulp-imagemin": "3.0.3",
    "gulp-less": "1.3.3",
    "gulp-live-server": "0.0.31",
    "gulp-minify-css": "0.3.7",
    "gulp-rev": "1.0.0",
    "gulp-sourcemaps": "1.1.1",
    "gulp-uglify": "0.3.1",
    "handlebars": "^4.0.5",
    "handlebars-helpers": "^0.10.0",
    "handlebars-loader": "^1.3.0",
    "i18n-abide": "0.0.26",
    "ibp-explorer-data-client": "git+https://github.com/okfn/ibp-explorer-data-client.git#v1.3.3",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-pngquant": "^5.0.0",
    "imports": "^1.0.0",
    "imports-loader": "^0.6.5",
    "jquery": "^2.1.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "linkifyjs": "^2.1.4",
    "metalsmith": "^2.3.0",
    "metalsmith-json-to-files": "brew/metalsmith-json-to-files#json-from-metadata",
    "metalsmith-layouts": "^1.8.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "request": "~2.81.0",
    "resolve-url-loader": "^1.6.0",
    "run-sequence": "^1.2.2",
    "scrolltofixed": "1.0.6",
    "slug-component": "^1.1.0",
    "slugify": "^1.4.0",
    "style-loader": "^0.13.1",
    "swig": "~1.4.2",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "whatwg-fetch": "^1.0.0",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.10.4",
    "babel-istanbul": "^0.12.2",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "9.4.1",
    "jsdom-global": "2.0.0",
    "mocha": "^7.1.0",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^3.10.3",
    "zombie": "^5.0.5"
  },
  "directories": {
    "test": "tests"
  }
}
