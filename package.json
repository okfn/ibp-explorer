    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "0.14.0",
    "request": "^2.74.0",
    "resolve-url-loader": "^1.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
