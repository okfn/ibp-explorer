{
  "author": "Tom Rees",
  "name": "open-budget-explorer",
  "description": "Online explorer and reporting tool for the Open Budget Survey results",
  "version": "1.0.0",
  "homepage": "http://internationalbudget.org",
  "repository": {
    "type": "git",
    "url": "http://github.com/okfn/dpkg-ibp"
  },
  "engines": {
    "node": "~6.3.0",
    "npm": "3.10.5"
  },
  "scripts": {
    "start": "node bin/www",
    "build:dev:explorer": "webpack --config webpack.config.development.js",
    "build:dev:explorer:watch": "webpack --config webpack.config.development.js --watch",
    "build:dev:tracker": "gulp build:dev:tracker",
    "build:dev:tracker:watch": "gulp tracker-watch",
    "build:prod:explorer": "webpack --config webpack.config.production.js",
    "build:prod:tracker": "gulp build:prod:tracker",
    "build:dev:watch": "gulp dev:watch",
    "build:dev": "npm run build:dev:explorer && npm run build:dev:tracker",
    "build:prod": "npm run build:prod:explorer && npm run build:prod:tracker",
    "build:prod:start": "npm run build:prod && npm run start",
    "test": "mocha --require babel-core/register tests/",
    "coverage": "babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha -- --report lcovonly --require babel-core/register tests/*.js && if [ -f ./coverage/lcov.info ]; then cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage; fi"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "body-parser": "~1.0.0",
    "bower": "1.3.8",
    "cookie-parser": "~1.0.1",
    "debug": "~0.7.4",
    "del": "0.1.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "gulp": "3.8.7",
    "gulp-clean": "0.3.1",
    "gulp-concat": "2.3.4",
    "gulp-extend": "0.2.0",
    "gulp-imagemin": "3.0.3",
    "gulp-less": "1.3.3",
    "gulp-live-server": "0.0.30",
    "gulp-minify-css": "0.3.7",
    "gulp-rev": "1.0.0",
    "gulp-sourcemaps": "1.1.1",
    "gulp-uglify": "0.3.1",
    "i18n-abide": "0.0.22",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-pngquant": "^5.0.0",
    "jquery": "^2.1.3",
    "moment": "2.8.1",
    "morgan": "~1.0.0",
    "open-budget-indaba-client": "git+https://github.com/okfn/open-budget-indaba-client.git",
    "request": "~2.39.0",
    "swig": "~1.4.2",
    "underscore": "^1.3.3",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "async": "^2.0.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.10.4",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "expect.js": "0.1.2",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fastparse": "^1.1.1",
    "file-loader": "^0.9.0",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.3.0",
    "imports": "^1.0.0",
    "imports-loader": "^0.6.5",
    "jsdom": "9.4.1",
    "jsdom-global": "2.0.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.0",
    "request": "^2.74.0",
    "resolve-url-loader": "^1.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  },
  "directories": {
    "test": "tests"
  }
}
